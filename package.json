{
  "name": "apoco",
  "version": "0.1.3",
  "description": "Single Page Web Framework",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint -f compact --env browser *.js",
    "eslint:test": "eslint -f compact --config test/eslint.conf --env mocha test/*.js",
    "pretest": "npm run eslint && npm run eslint:test",
    "test": "cross-env NODE_ENV=test mocha -R mocha-tldr-reporter -C test",
    "posttest": "rimraf .__browserify_string_empty.js",
    "apoco": "browserify  ./index.js -o apoco.js"
  },
  "author": "Val Wardlaw <val@perluceo.com>",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^13.0.0",
    "browserify-string": "^1.1.1",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.7.0",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "mocha-tldr-reporter": "^1.0.0",
    "rimraf": "^2.5.2",
    "selenium-webdriver": "^2.53.2",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "clone-deep": "^0.2.4"
  }
}
